	}
});
$("#mainCheckMe").live('change',function(){
	var stateid=$(this).attr('stateidmain');
	if($(this).is(':checked')) {
		$("#ULIST"+$(this).attr('stateidmain')).each(function(){
			$('input[name=checkMe'+stateid+']').prop('checked', true);
		});
	}
	else
	{
		$("#ULIST"+$(this).attr('stateidmain')).each(function(){
			$('input[name=checkMe'+stateid+']').prop('checked', false);
		});
	}
});


