package com.mars.distribution.ServicePojo;



import java.util.Collection;

import javax.xml.bind.annotation.XmlRootElement;

import com.mars.distribution.model.Branch;

@XmlRootElement(name="BranchList")
public class BranchCreationPojo {
	private String zoneName;
	
	 private BranchCreationPojo branchPojo;
	 
	public String getZoneName() {
		return zoneName;
	}
	public BranchCreationPojo getBranchPojo() {
		return branchPojo;
	}
	public void setBranchPojo(BranchCreationPojo branchPojo) {
		this.branchPojo = branchPojo;
	}
	public void setZoneName(String zoneName) {
		this.zoneName = zoneName;
	}
	public String getRegionName() {
		return regionName;
	}
	public void setRegionName(String regionName) {
		this.regionName = regionName;
	}
	public Collection<Branch> getBranches() {
		return branches;
	}
	public void setBranches(Collection<Branch> branches) {
		this.branches = branches;
	}
	public Collection<BranchCreationPojo> getBranchpojo() {
		return branchpojo;
	}
	public void setBranchpojo(Collection<BranchCreationPojo> branchpojo) {
		this.branchpojo = branchpojo;
	}
	private String regionName;
	 private Collection<Branch> branches;
	 private Collection<BranchCreationPojo> branchpojo;
	

}
